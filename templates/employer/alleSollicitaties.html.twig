{% extends 'base.html.twig' %}

{% block title %} :: Sollicitatieoverzicht{% endblock %}



{% block body %}


    Sollicitaties bij {{ user.naam }}

    {% for vacature in user.vacatures %}

        <pre>
            {{ vacature.vacaturetitel }}

            <pre>

                {% for sollicitatie in vacature.sollicitaties %}

                {{ sollicitatie.user.voornaam }} {{ sollicitatie.user.naam }} heeft hierop gesolliciteerd!
                    
                {% endfor %}
            
            </pre>
            

        </pre>
    
    {% endfor %}


{% endblock %}
